@model StudentData




<div class="row pb-1 mt-2">
    <h2 class="text-primary">Update Student Details</h2>
    <hr />
</div>
<form method="post">
    <div class="border p-2 mt-1">
        <div class="form-group">
            <label asp-for="FirstName" class="inline">First name</label>
            <input asp-for="FirstName" disabled class="form-control p-1" />
        </div>
        <div class="form-group">
            <label asp-for="LastName">Lastname</label>
            <input asp-for="LastName" disabled class="form-control p-1" />

        </div>
        <div class="form-group">
            <label asp-for="Course">Course</label>
            <input asp-for="Course" disabled class="form-control p-1" />

        </div>
        <div class="form-group">
            <label asp-for="Gender">Gender</label>
            <input asp-for="Gender" disabled class="form-control p-1" />

        </div>
        <div class="form-group">
            <label asp-for="Email">Email</label>
            <input asp-for="Email" disabled class="form-control p-1" />

        </div>
        <div class="form-group2">
            <label asp-for="Phone">Phone</label>
            <input asp-for="Phone" disabled class="form-control p-1" />

            <div class="form-group">
                <label asp-for="Password">Password</label>
                <input asp-for="Password" type="password" class="form-control p-1" />
                <span asp-validation-for="Password" class="text-danger small"></span>
            </div>
            <button type="submit" class="btn btn-primary mt-2">Delete</button>
            <a asp-controller="StudentData" asp-action="Delete" class="btn btn-light float-end mt-2">List Student</a>

        </div>
</form>

